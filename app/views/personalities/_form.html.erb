<%= simple_form_for personality do |f| %>
  <%= f.input :first_name %>
  <%= f.input :last_name %>
  <%= f.input :bio %>
  <%= f.input :desired_gender %>
  <%= f.input :desired_age %>
  <%= f.input :picture %>
  <%= f.input :picture_cache, as: :hidden %>
  <%= f.input :price_per_day %>
  <%= f.submit "Add personaility" %>
<% end %>

